{
    "name": "@univerjs/preset-sheets-node-core",
    "version": "0.5.0",
    "private": false,
    "description": "",
    "author": "DreamNum <developer@univer.ai>",
    "license": "Apache-2.0",
    "funding": {
        "type": "opencollective",
        "url": "https://opencollective.com/univer"
    },
    "homepage": "https://univer.ai",
    "repository": {
        "type": "git",
        "url": "https://github.com/dream-num/univer-presets"
    },
    "bugs": {
        "url": "https://github.com/dream-num/univer-presets/issues"
    },
    "keywords": [
        "univer"
    ],
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*",
        "./worker": "./src/worker.ts",
        "./locales/*": "./src/locales/*.ts"
    },
    "publishConfig": {
        "access": "public",
        "main": "./lib/es/index.js",
        "module": "./lib/es/index.js",
        "exports": {
            ".": {
                "import": "./lib/es/index.js",
                "require": "./lib/cjs/index.js",
                "types": "./lib/types/index.d.ts"
            },
            "./*": {
                "import": "./lib/es/*",
                "require": "./lib/cjs/*",
                "types": "./lib/types/index.d.ts"
            },
            "./locales/*": {
                "import": "./lib/es/locales/*.js",
                "require": "./lib/cjs/locales/*.js",
                "types": "./lib/types/locales/*.d.ts"
            },
            "./lib/*": "./lib/*"
        }
    },
    "main": "./src/index.ts",
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "prebuild": "tsx prepare.ts",
        "build:preset": "tsx build.ts"
    },
    "peerDependencies": {
        "react": "^16.9.0 || ^17.0.0 || ^18.0.0",
        "rxjs": ">=7.0.0"
    },
    "dependencies": {
        "@univerjs/docs": "0.5.0",
        "@univerjs/engine-formula": "0.5.0",
        "@univerjs/engine-render": "0.5.0",
        "@univerjs/rpc-node": "0.5.0",
        "@univerjs/sheets": "0.5.0",
        "@univerjs/sheets-data-validation": "0.5.0",
        "@univerjs/sheets-drawing": "0.5.0",
        "@univerjs/sheets-filter": "0.5.0",
        "@univerjs/sheets-formula": "0.5.0",
        "@univerjs/sheets-hyper-link": "0.5.0",
        "@univerjs/sheets-numfmt": "0.5.0",
        "@univerjs/sheets-sort": "0.5.0",
        "@univerjs/sheets-thread-comment": "0.5.0",
        "@univerjs/thread-comment": "0.5.0"
    },
    "devDependencies": {
        "@univerjs-infra/shared": "workspace:*",
        "@univerjs/core": "0.5.0",
        "typescript": "^5.7.2"
    }
}
