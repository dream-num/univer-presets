{
    "name": "@univerjs/preset-sheets-core",
    "version": "0.8.0-beta.0",
    "private": false,
    "description": "",
    "author": "DreamNum <developer@univer.ai>",
    "license": "Apache-2.0",
    "funding": {
        "type": "opencollective",
        "url": "https://opencollective.com/univer"
    },
    "homepage": "https://univer.ai",
    "repository": {
        "type": "git",
        "url": "https://github.com/dream-num/univer-presets"
    },
    "bugs": {
        "url": "https://github.com/dream-num/univer-presets/issues"
    },
    "keywords": [
        "univer"
    ],
    "sideEffects": [
        "*.css"
    ],
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*",
        "./worker": "./src/worker.ts",
        "./locales/*": "./src/locales/*.ts"
    },
    "publishConfig": {
        "access": "public",
        "main": "./lib/es/index.js",
        "module": "./lib/es/index.js",
        "exports": {
            ".": {
                "import": "./lib/es/index.js",
                "require": "./lib/cjs/index.js",
                "types": "./lib/types/index.d.ts"
            },
            "./*": {
                "import": "./lib/es/*",
                "require": "./lib/cjs/*",
                "types": "./lib/types/index.d.ts"
            },
            "./worker": {
                "import": "./lib/es/worker.js",
                "require": "./lib/cjs/locales/*.js",
                "types": "./lib/types/worker.d.ts"
            },
            "./locales/*": {
                "import": "./lib/es/locales/*.js",
                "require": "./lib/cjs/locales/*.js",
                "types": "./lib/types/locales/*.d.ts"
            },
            "./lib/*": "./lib/*"
        }
    },
    "main": "./src/index.ts",
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "prebuild:preset": "tsx prepare.ts",
        "build:preset": "tsx build.ts",
        "lint:types": "tsc --noEmit"
    },
    "peerDependencies": {
        "react": "^16.9.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^19.0.0-rc",
        "react-dom": "^16.9.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^19.0.0-rc",
        "rxjs": ">=7.0.0"
    },
    "dependencies": {
        "@univerjs/design": "0.8.0-beta.0",
        "@univerjs/docs": "0.8.0-beta.0",
        "@univerjs/docs-ui": "0.8.0-beta.0",
        "@univerjs/engine-formula": "0.8.0-beta.0",
        "@univerjs/engine-numfmt": "0.8.0-beta.0",
        "@univerjs/engine-render": "0.8.0-beta.0",
        "@univerjs/network": "0.8.0-beta.0",
        "@univerjs/rpc": "0.8.0-beta.0",
        "@univerjs/sheets": "0.8.0-beta.0",
        "@univerjs/sheets-formula": "0.8.0-beta.0",
        "@univerjs/sheets-formula-ui": "0.8.0-beta.0",
        "@univerjs/sheets-numfmt": "0.8.0-beta.0",
        "@univerjs/sheets-numfmt-ui": "0.8.0-beta.0",
        "@univerjs/sheets-ui": "0.8.0-beta.0",
        "@univerjs/ui": "0.8.0-beta.0"
    },
    "devDependencies": {
        "@univerjs-infra/shared": "workspace:*",
        "@univerjs/core": "0.8.0-beta.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "rxjs": "7.8.2",
        "typescript": "^5.8.3"
    }
}
