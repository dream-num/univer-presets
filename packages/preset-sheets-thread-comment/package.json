{
    "name": "@univerjs/preset-sheets-thread-comment",
    "version": "0.6.4",
    "private": false,
    "description": "",
    "author": "DreamNum <developer@univer.ai>",
    "license": "Apache-2.0",
    "funding": {
        "type": "opencollective",
        "url": "https://opencollective.com/univer"
    },
    "homepage": "https://univer.ai",
    "repository": {
        "type": "git",
        "url": "https://github.com/dream-num/univer-presets"
    },
    "bugs": {
        "url": "https://github.com/dream-num/univer-presets/issues"
    },
    "keywords": [
        "univer"
    ],
    "sideEffects": [
        "*.css"
    ],
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*",
        "./locales/*": "./src/locales/*.ts"
    },
    "main": "./src/index.ts",
    "publishConfig": {
        "access": "public",
        "main": "./lib/es/index.js",
        "module": "./lib/es/index.js",
        "exports": {
            ".": {
                "import": "./lib/es/index.js",
                "require": "./lib/cjs/index.js",
                "types": "./lib/types/index.d.ts"
            },
            "./*": {
                "import": "./lib/es/*",
                "require": "./lib/cjs/*",
                "types": "./lib/types/index.d.ts"
            },
            "./locales/*": {
                "import": "./lib/es/locales/*.js",
                "require": "./lib/cjs/locales/*.js",
                "types": "./lib/types/locales/*.d.ts"
            },
            "./lib/*": "./lib/*"
        }
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "prebuild:preset": "tsx prepare.ts",
        "build:preset": "tsx build.ts",
        "lint:types": "tsc --noEmit"
    },
    "peerDependencies": {
        "react": ">=16.9.0",
        "react-dom": ">=16.9.0",
        "rxjs": ">=7.0.0"
    },
    "dependencies": {
        "@univerjs/sheets-thread-comment": "0.6.5",
        "@univerjs/sheets-thread-comment-ui": "0.6.5",
        "@univerjs/thread-comment": "0.6.5",
        "@univerjs/thread-comment-ui": "0.6.5"
    },
    "devDependencies": {
        "@univerjs-infra/shared": "workspace:*",
        "@univerjs/core": "0.6.5",
        "typescript": "^5.8.2"
    }
}
